{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["type", "protocol_version", "ack_for", "accepted"],
  "properties": {
    "type": {"const": "ACK"},
    "protocol_version": {"type": "string", "enum": ["1.1"]},
    "ack_for": {"type": "string"},
    "accepted": {"type": "boolean"},
    "code": {"type": "string"},
    "message": {"type": "string"},
    "server_tick": {"type": "integer", "minimum": 0},
    "world_id": {"type": "string"}
  },
  "additionalProperties": true
}
